let i[2] = { 1, 2 }: int;
let j[2] = { false, true }: bool;

def array_with_expr_index() {
  let i[3] = { 3, 4, 5 }: int;
  let k: int;
  write i[k], i[k + 1];
  return;
}

def main(): int {
  let i[2] = { 3, 4 }: int;
  let j[2] = { false, true }: bool;

  write i[0], i[1];
  write j[0], j[1];

  array_with_expr_index();
  return 0;
}
